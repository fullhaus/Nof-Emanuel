pipeline {
    agent {label 'localhost'}
    stages {
        stage('Checkout code') {
            steps {
                echo 'Checkout code'
            }
        }
        stage('Build docker image') {
            steps {
                echo 'Build docker image'
            }
        }
        stage('Deploy to AKS') {
            steps {
                echo 'Deploy to AKS'
            }
        }
    }
}